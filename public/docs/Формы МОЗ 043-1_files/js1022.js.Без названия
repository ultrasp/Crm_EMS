    };

    this.formatCustomProductsInSelect2 = function (data) {
        return stockActions.formatPhotoInSelect2(data, stockActions.SELECT2_TYPE_PRODUCT);

    };

    this.formatCustomProductsSelectedItemInSelect2 = function (data) {
